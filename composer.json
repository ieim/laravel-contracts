{
    "name": "ieim/laravel-contracts",
    "type": "library",
    "description": "Set of interfaces which help me to structure laravel projects.",
    "keywords": [
        "ieim",
        "laravel-contracts"
    ],
    "homepage": "https://github.com/ieim/laravel-contracts",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Eichenseher",
            "email": "this@marceichenseher.de",
            "homepage": "https://marceichenseher.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "laravel/framework": "^5.8.0",
        "squizlabs/php_codesniffer": "^3.0.0",
        "vimeo/psalm": "^3.13"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ieim\\LaravelContracts\\": "src"
        }
    },
    "scripts": {
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "test": "phpunit"
    }
}

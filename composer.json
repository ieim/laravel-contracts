{
    "name": "ieim/laravel-contracts",
    "type": "library",
    "description": "Set of interfaces which help me to structure laravel projects.",
    "keywords": [
        "ieim",
        "laravel-contracts"
    ],
    "homepage": "https://github.com/ieim/laravel-contracts",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Eichenseher",
            "email": "this@marceichenseher.de",
            "homepage": "marceichenseher.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ieim\\LaravelContracts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ieim\\LaravelContracts\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
